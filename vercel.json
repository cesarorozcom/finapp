{
  "version": 2,
  "builds": [
    {
      "src": "budget_tracker/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/budget_tracker/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/budget_tracker/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "budget_tracker.settings"
  },
  "functions": {
    "budget_tracker/wsgi.py": {
      "maxDuration": 30
    }
  }
}